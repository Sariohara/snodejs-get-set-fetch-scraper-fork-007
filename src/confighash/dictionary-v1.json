{
  "url": "https://.com/.html",
  "scenario": "browser-static-content|dom-static-content",
  "pluginOpts": [
    {
      "name": "ExtractHtmlContentPlugin",
      "selectorPairs": [
        {
          "label": "",
          "contentSelector": "",
          "contentProperty": "innerText"
        }
      ],
      "domRead": true
    },
    {
      "name": "ExtractUrlsPlugin",
      "selectorPairs": [
        { 
          "urlSelector": "a",
          "titleSelector": ""
        }
      ],
      "maxDepth": -1
    },
    {
      "name": "BrowserFetchPlugin|NodeFetchPlugin",
      "stabilityCheck": 0,
      "stabilityTimeout": 0
    },
    {
      "name": "InsertResourcesPlugin",
      "maxResources": -1
    },
    {
      "name": "ScrollPlugin",
      "domWrite": true,
      "delay": 1000,
      "maxActions": -1
    },
    {
      "name": "SelectResourcePlugin",
      "frequency": -1,
      "delay": 1000
    },
    {
      "name": "UpsertResourcePlugin"
    }
  ]
}
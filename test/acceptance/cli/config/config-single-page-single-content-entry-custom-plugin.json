{
  "storage": {
    "client": "sqlite3",
    "useNullAsDefault": true,
    "connection": {
      "filename": "../../../tmp/db.sqlite"
    }
  },
  "dom": {
    "client": "cheerio"
  },
  "scrape": {
    "name": "sitea.com",
    "pipeline": "dom-static-content",
    "pluginOpts": [
      {
        "name": "NodeFetchPlugin",
        "headers": {
          "Accept-Encoding": "identity"
        }
      },
      {
        "name": "H1CounterPlugin",
        "path": "../plugins/h1-counter-plugin.js",
        "replace": "ExtractHtmlContentPlugin",
        "startVal": 50
      }
    ],
    "resources": [
      {
        "url": "http://sitea.com/index.html"
      }
    ]
  },
  "concurrency": {
    "proxy": {
      "maxRequests": 10,
      "delay": 100
    },
    "domain": {
      "maxRequests": 10,
      "delay": 100
    },
    "proxyPool": [ {
      "host": "127.0.0.1",
      "port": 8080
    } ]
  },
  "process": {
  }
}
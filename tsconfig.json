{
  "compilerOptions": {
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "target": "esnext",
    "strict": false,
    "moduleResolution": "node",
    "module": "commonjs",
    "allowJs": true,
    "rootDir": "src",
    "outDir": "dist",
    "declaration": true,
    "newLine": "LF"
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
  
}